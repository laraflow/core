{
    "name": "laraflow/core",
    "description": "set of wrapper class for laraflow apps",
    "keywords": [
        "laraflow",
        "laravel",
        "core"
    ],
    "homepage": "https://github.com/laraflow/core",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammad Hafijul Islam",
            "email": "hafijul233@gmail.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^7.4|^8.1",
        "ext-pdo": "*",
        "illuminate/contracts": "^8.83",
        "intervention/image": "^2.7",
        "laravolt/avatar": "^4.1",
        "maatwebsite/excel": "^3.1",
        "rap2hpoutre/fast-excel": "^3.2",
        "spatie/laravel-medialibrary": "^9.12",
        "spatie/laravel-package-tools": "^1.9.2",
        "yoeunes/notify": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "nunomaduro/collision": "^5.11",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.24",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Laraflow\\Core\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laraflow\\Core\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laraflow\\Core\\LaraflowServiceProvider",
                "Laraflow\\Core\\MoneyServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
