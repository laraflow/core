{
    "name": "laraflow/core",
    "description": "set of wrapper class for laraflow apps",
    "keywords": [
        "laraflow",
        "laravel",
        "core"
    ],
    "homepage": "https://github.com/laraflow/core",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammad Hafijul Islam",
            "email": "hafijul233@gmail.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^7.4|^8.1",
        "ext-bcmath": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "ext-xml": "*",
        "ext-xmlreader": "*",
        "ext-zip": "*",
        "ext-zlib": "*",
        "illuminate/contracts": "^8.83",
        "intervention/image": "^2.7",
        "laravolt/avatar": "^4.1",
        "maatwebsite/excel": "^3.1",
        "php-flasher/flasher-laravel": "^1.7",
        "psr/simple-cache": "^1.0",
        "rap2hpoutre/fast-excel": "^4.1",
        "spatie/laravel-medialibrary": "^9.12"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.11",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.24",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Laraflow\\Core\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laraflow\\Core\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laraflow\\Core\\CoreServiceProvider",
                "Laraflow\\Core\\MoneyServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
